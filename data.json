[{"model": "hotelhub.about", "pk": 1, "fields": {"title": "Welcome to Our Luxurious Hotel", "description": "Our hotel offers an exceptional stay in a serene and luxurious environment. Located in the heart of the city, we pride ourselves on offering world-class amenities, personalized services, and exquisite rooms with breathtaking views. Whether you're here for business or leisure, we provide a comfortable and memorable experience. Our dedicated staff ensures every guest feels at home, offering bespoke services and thoughtful touches that make your stay truly special.", "image": "about/istockphoto-472899538-612x612.jpg"}}, {"model": "hotelhub.service", "pk": 1, "fields": {"name": "24/7 Concierge Service", "description": "Our 24/7 concierge service ensures that all your needs are met at any time of the day or night. Whether you require restaurant reservations, transportation arrangements, or personalized recommendations, our dedicated team is here to make your stay hassle-free and memorable.", "icon": "services/images_2.png"}}, {"model": "hotelhub.room", "pk": 1, "fields": {"name": "Garden Deluxe Room", "description": "A beautifully decorated room with views of our lush garden, perfect for a relaxing stay. The room includes a queen-size bed, a work desk, and a cozy seating area.", "price": "220.00", "image": "rooms/download_1.jpg"}}, {"model": "hotelhub.room", "pk": 2, "fields": {"name": "Mountain View Suite", "description": "This luxurious suite offers breathtaking mountain views, a large king-sized bed, a private balcony, and a cozy seating area. The suite is equipped with all modern amenities, including a flat-screen TV, high-speed Wi-Fi, and a well-appointed bathroom with a rain shower. Ideal for guests seeking relaxation and tranquility in a stunning natural setting", "price": "200.00", "image": "rooms/download_2.jpg"}}, {"model": "hotelhub.team", "pk": 1, "fields": {"name": "John Doe", "position": "General Manager", "image": "team/team_2.jpg", "bio": "John Doe is the General Manager of our hotel with over 15 years of experience in the hospitality industry. He is passionate about delivering exceptional guest experiences and ensuring the smooth operations of the hotel. Under his leadership, the hotel has won multiple awards for outstanding customer service."}}, {"model": "hotelhub.testimonial", "pk": 1, "fields": {"name": "Jane Smith", "review": "I had an incredible experience at this hotel! The staff was friendly, the rooms were spotless, and the amenities were top-notch. I highly recommend this place to anyone looking for a luxurious stay.", "ratings": 5, "image": "testimonials/download_3.jpg"}}, {"model": "hotelhub.booking", "pk": 1, "fields": {"room": 1, "guest_name": "Audrey Njeri", "email": "audreymbora@gmail.com", "check_in_date": "2024-12-15", "check_out_date": "2024-12-25", "message": "\"We’re excited to host you! Please provide your room preference, name, and email, along with your check-in and check-out dates. If you have any special requests or additional details, feel free to include them in the message box. We’ll ensure your stay is as comfortable and enjoyable as possible!\"", "ratings": 5}}, {"model": "hotelhub.contact", "pk": 1, "fields": {"name": "Audrey Njeri", "email": "audreymbora@gmail.com", "subject": "Inquiry about Room Availability for December", "message": "\"Dear Team,\r\nI would like to inquire about the availability of rooms at your hotel for the dates from December 15th to December 25th. Could you please provide details on the rates and amenities available? Additionally, I would appreciate it if you could share any promotional offers or packages for those dates.\r\nLooking forward to hearing from you soon.\r\nBest regards,\r\nAudrey Njeri\""}}, {"model": "hotelhub.slider", "pk": 1, "fields": {"title": "Experience Luxury Like Never Before", "subtitle": "Indulge in world-class amenities and stunning views at our exclusive hotel.", "image": "slider/istockphoto-903417402-612x612.jpg", "link": "http